alias dm="docker-machine"
alias dmi="dm ip"
alias dme="dm env"
alias dms="dm start && dme"
alias dmR="dm rm default"
alias dmc="dm create --driver virtualbox default"
alias dc="docker-compose"
alias dcu="dc up"
alias dck="dc kill"
alias dcR="dc rm -f"
alias dckR="dck && dcR"
alias d="docker"
alias ds="d start"
alias dS="d stop"
alias dr="d restart"
alias dl="d log"
alias di="d images"
alias diRu="d rmi $(docker images | grep '^<none>' | awk '{print $3}')"
alias diR="d rmi -f $(di -aq)"
alias dps="d ps"
alias dex="d exec"
alias nwtmenu="d run -it --rm  -v ~/.ssh/ansible.pem:/root/.ssh/id_rsa -v ~/.ssh/known_hosts:/root/.ssh/known_hosts reg.nwt.se/nwt/nwtmenu"
